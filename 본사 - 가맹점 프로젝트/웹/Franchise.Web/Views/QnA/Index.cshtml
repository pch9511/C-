@model IEnumerable<QnA>

<div class="row">
    <div class="col-md-6">
        <h2>자주묻는 질문</h2>
        <table style="width: 900px; margin-right:auto;">
            <tr>
                <td>
                    <table class="table table-bordered table-hover">
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td style="height:100px; font-size:30px;">
                                    <a asp-area="" asp-controller="QnA" asp-action="QnADetail" asp-route-id =@item.ChatLog>@item.Question</a>
                                </td>
                            </tr>
                        }
                    </table>
                </td>
            </tr>
        </table>
    </div>
</div>
